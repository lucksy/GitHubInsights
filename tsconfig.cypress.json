{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress", "node"],
      "moduleResolution": "node",
      "esModuleInterop": true,
      "sourceMap": false,
      "allowJs": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      }
    },
    "include": ["cypress/**/*.ts"],
    "exclude": ["node_modules"]
  }